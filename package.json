{
  "name": "employee-data-project",
  "version": "1.0.0",
  "description": "Employee data",
  "scripts": {
    "install:all": "npm install && cd server && npm install && cd ../vite-react-table && npm install",
    "start:server": "cd server && npm run dev",
    "start:client": "cd vite-react-table && npm run dev",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
